@model Escuela

@{
    ViewData["Title"] = "Informacion Escuela";
}

<h1 class="display-4">Escuela @ViewData["Title"]</h1>
<h2>Nombre: @Model.Nombre </h2>
<h3>Tipo de Escuela: @Model.TipoEscuela </h3>
<address>
    <p>Direccion: @Model.Dirección </p>
    <p>Pais: @Model.Pais </p>
    <p>Ciudad @Model.Ciudad </p>
</address>
<p><strong>Año fundacion: @Model.AñoDeCreación </strong></p>

